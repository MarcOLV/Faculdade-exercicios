algoritmo exercicio2

	inteiro dependentes
	caractere nome, cargo
	inicio	
	escreva("digite seu nome:")
	leia(nome)
	escreva("digite a quantidade de dependentes:")
	leia(dependentes)
	escreva("digite seu cargo:")
	leia(cargo)
	escreva("Nome: " + nome + "Quantidade de dependentes: " + dependentes + "Cargo: " + cargo)
	
	fim