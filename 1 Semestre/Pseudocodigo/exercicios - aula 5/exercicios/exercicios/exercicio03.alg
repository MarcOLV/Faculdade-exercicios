algoritmo exercicio03
	real bolsa, qtndt
	inicio
		escreva("Digite o valor da bolsa:")
		leia(bolsa)
		escreva("Digite o tempo de estagio, em anos:")
		leia(qtndt)
		
		se (bolsa >= 750 e bolsa <=950 e qtndt >=2)
			escreva("Voce vai participar do treinamento")
		senao
			escreva("NÃ£o participara do treinamento")
			
	fim