algoritmo exercicio1
    início
    real  valortotal, preco
    inteiro qtdIngresso

    escreva("Qual o valor do espetaculo: ")
    leia(valortotal)
    escreva("Qual o valor do ingresso: ")
    leia(preco)

    qtdIngreso = valortotal / preco

    escreva("A quantidade de ingressos para o valor ser abatido será de: " + qtdIngreso)
    
    fim
	
