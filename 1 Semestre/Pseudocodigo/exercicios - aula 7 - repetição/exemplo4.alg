algoritmo exemplo4
	real nota1,nota2,media
	literal aluno
	inteira i
	inicio
		i = 0
		enquanto(i < 10){
			escreva("Digite o nome do aluno: ")
			leia(aluno)
			escreva("Digite a primeira nota: ")
			leia(nota1)
			escreva("Digite a segunda nota: ")
			leia(nota2)
			media=(nota1+nota2)/2
			escreva("A media do aluno " + aluno + " Ã©: " + media)
		i++
		}
	
	fim
    
  
  
